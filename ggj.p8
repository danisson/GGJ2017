pico-8 cartridge // http://www.pico-8.com
version 8
__lua__
-- data structure representing a player
player = {}
-- current action
-- 1 : resting
-- 2 : hands to the left
-- 3 : hands to the right
-- 4 : hands up
-- 5 : crouching
player.action = 1
-- current sprite. for now it corresponds to the action
player.sprite = 1

-- sound timer control
accum_dt = 0
update_called = false
correct_input = false

function _init()
	init_timers()
	add_timer(
		"sound",
		30,
		function(dt,elapsed,length,timer)
			accum_dt += dt
			if accum_dt >= 0.200 then
				accum_dt = 0
				update_called = true
				sfx(0)
			else
				update_called = false
			end
		end,
		function()
			restart_timer()
		end
		)
end

function _update()
	local input = false
	if btn(0) then
		player.action = 2
		input = true
	end
	if btn(1) then
		player.action = 3
		input = true
	end
	if btn(2) then
		player.action = 4
		input = true
	end
	if btn(3) then
		player.action = 5
		input = true
	end

	if input then 
		player.sprite = player.action
		-- starts timer for returning to rest position
		add_timer(
			"rest",
			0.2,
			nil,
			function()
				player.action = 1
				player.sprite = player.action
			end
		)
	end

	if input and update_called then
		correct_input = true	
	end

	update_timers()
end

function _draw()
	cls()
	if correct_input then
		print(":D")
		correct_input = false
	end
	spr(player.sprite, 64, 64)
end

-- timers api. from: http://www.lexaloffle.com/bbs/?tid=3202

local timers = {}
local last_time = nil

function init_timers ()
  last_time = time()
end

function add_timer (name,
    length, step_fn, end_fn,
    start_paused)
  local timer = {
    length=length,
    elapsed=0,
    active=not start_paused,
    step_fn=step_fn,
    end_fn=end_fn
  }
  timers[name] = timer
  return timer
end

function update_timers ()
  local t = time()
  local dt = t - last_time
  last_time = t
  for name,timer in pairs(timers) do
    if timer.active then
      timer.elapsed += dt
      local elapsed = timer.elapsed
      local length = timer.length
      if elapsed < length then
        if timer.step_fn then
          timer.step_fn(dt,elapsed,length,timer)
        end
    else
        timer.active = false
        if timer.end_fn then
          timer.end_fn(dt,elapsed,length,timer)
        end
      end
    end
  end
end

function restart_timer (name, start_paused)
  local timer = timers[name]
  if (not timer) return
  timer.elapsed = 0
  timer.active = not start_paused
end

